h1 Inbox

p = link_to 'View past items assigned to me', archive_path

table.table.table-striped
  thead
    tr
      th Date
      th Description
      th Original Desc.
      th Category
      th Amount
      th Type
      th Account
  tbody
    - @items.each do |item|
      tr
        td = item.date
        td = item.desc
        td = item.desc_orig
        td = item.category
        td = item.cents
        td = item.txn_type
        td = item.account
